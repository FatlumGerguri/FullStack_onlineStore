ad.quantity;\r\n  cart.push(data);\r\n  commit('setCart', cart);\r\n}\r\n\r\n// Poista valitsemasi tuotteet ostoskorista\r\nexport function removeCart({commit, getters}, id) {\r\n  let cart = [];\r\n  if (id) {\r\n    for (let index = 0; index < getters.cart.length; index++) {\r\n      const element = getters.cart[index];\r\n      if (element.id !== id) {\r\n        cart.push(element);\r\n      }\r\n    }\r\n  }\r\n  commit('setCart', cart);\r\n}\r\n"]}]}